<div class="col-lg-12">
	<section class="row">
		<div class="col-lg-12">
			<div class="card mb-4">
				<div class="card-block">
					<h3 class="card-title">{{t model.title}}</h3>
					<table id="configurations-table" class="table table-striped table-bordered"
						cellspacing="0" width="100%">
						<thead>
							<tr>
								<th>Name</th>
								<th>Id</th>
							</tr>
						</thead>
						<tbody>
							{{#each model.networkConfigurations as | configuration |}}
							<tr>
							
							
								<td>{{#link-to "network-configurations.network-configuration"
							configuration.id}}{{configuration.name}}{{/link-to}} </td>
								<td>{{configuration.id}}</td>
							
							</tr>{{/each}}
						</tbody>
					</table>
					
				</div>
				<div class="card-footer">
					<button type="button" class="btn btn-md btn-secondary" {{action "newConfiguration"}}>{{t "route.common.new" }}</button>
				</div>
			</div>
		</div>
	</section>
</div>

{{outlet}}
